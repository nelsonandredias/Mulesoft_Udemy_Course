  <xs:element xmlns:mule="http://www.mulesoft.org/schema/mule/core" substitutionGroup="mule:abstract-shared-extension" name="config">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="mule:abstractExtensionType">
          <xs:annotation>
            <xs:documentation>Base configuration for JmsConnector</xs:documentation>
          </xs:annotation>
          <xs:sequence minOccurs="0" maxOccurs="1">
            <xs:element minOccurs="0" maxOccurs="1" ref="mule:abstractConnectionProvider"></xs:element>
            <xs:element minOccurs="0" maxOccurs="1" ref="mule:expiration-policy"></xs:element>
            <xs:element minOccurs="0" maxOccurs="1" name="consumer-config">
              <xs:complexType>
                <xs:complexContent>
                  <xs:extension base="mule:abstractExtensionType">
                    <xs:sequence>
                      <xs:element minOccurs="0" maxOccurs="1" name="consumer-type">
                        <xs:annotation>
                          <xs:documentation>The ConsumerType to be used by default when consuming a Message Can be overridden at the message source level.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                          <xs:choice minOccurs="1" maxOccurs="1">
                            <xs:element xmlns:jms="http://www.mulesoft.org/schema/mule/jms" minOccurs="1" maxOccurs="1" ref="jms:abstract-consumer-type"></xs:element>
                            <xs:element xmlns:jms="http://www.mulesoft.org/schema/mule/jms" minOccurs="1" maxOccurs="1" ref="jms:global-abstract-consumer-type"></xs:element>
                          </xs:choice>
                        </xs:complexType>
                      </xs:element>
                    </xs:sequence>
                    <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/jms" type="org.mule.extensions.jms.api.ack.AckModeEnumType" use="optional" default="AUTO" name="ackMode">
                      <xs:annotation>
                        <xs:documentation>The ConsumerAckMode to use when consuming a Message Can be overridden at the message source level. This attribute has to be IMMEDIATE if transactionType is LOCAL or MULTI</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute type="xs:string" use="optional" name="selector">
                      <xs:annotation>
                        <xs:documentation>Default selector to be used for filtering when consuming a Message Can be overridden at the message source level.</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute type="mule:substitutableInt" use="optional" default="0" name="maxRedelivery">
                      <xs:annotation>
                        <xs:documentation>Used to configure the number of redelivers before discarding the message. No redelivery is represented with 0, while -1 means infinite re deliveries accepted.</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                  </xs:extension>
                </xs:complexContent>
              </xs:complexType>
            </xs:element>
            <xs:element minOccurs="0" maxOccurs="1" name="producer-config">
              <xs:complexType>
                <xs:complexContent>
                  <xs:extension base="mule:abstractExtensionType">
                    <xs:sequence></xs:sequence>
                    <xs:attribute type="mule:substitutableBoolean" use="optional" default="false" name="persistentDelivery">
                      <xs:annotation>
                        <xs:documentation>Sets the default value for the Message#getJMSDeliveryMode. If true, the DeliveryMode#PERSISTENT mode will be used, which instructs the JMS provider to take extra care to insure the message is not lost in transit due to a JMS provider failure.</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute type="mule:substitutableInt" use="optional" default="4" name="priority">
                      <xs:annotation>
                        <xs:documentation>Defines the default Message#getJMSPriority to use when sending messages</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute type="mule:substitutableLong" use="optional" default="0" name="timeToLive">
                      <xs:annotation>
                        <xs:documentation>Defines the default time the message will be in the broker before it expires and is discarded</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/jms" type="java.util.concurrent.TimeUnitEnumType" use="optional" default="MILLISECONDS" name="timeToLiveUnit">
                      <xs:annotation>
                        <xs:documentation>A TimeUnit which qualifies the #timeToLive attribute. &lt;p&gt; Defaults to MILLISECONDS</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute type="mule:substitutableBoolean" use="optional" default="false" name="disableMessageId">
                      <xs:annotation>
                        <xs:documentation>Defines the default value to use when producing messages, for disabling the Message#getJMSMessageID generation in the broker. Depending on the provider it may or may not have effect</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute type="mule:substitutableBoolean" use="optional" default="false" name="disableMessageTimestamp">
                      <xs:annotation>
                        <xs:documentation>Defines the default value to use, when producing messages, for disable Message#getJMSTimestamp generation in the broker. Depending on the provider it may or may not have effect.</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute type="mule:substitutableLong" use="optional" name="deliveryDelay">
                      <xs:annotation>
                        <xs:documentation>This is used to determine the Message delivery delay time which is calculated by adding the deliveryDelay value specified on the send method to the time the message was sent. &lt;p&gt; Only used in JmsSpecification#JMS_2_0</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/jms" type="java.util.concurrent.TimeUnitEnumType" use="optional" default="MILLISECONDS" name="deliveryDelayUnit">
                      <xs:annotation>
                        <xs:documentation>A TimeUnit which qualifies the #deliveryDelay attribute. &lt;p&gt; Defaults to MILLISECONDS</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute type="xs:string" use="optional" name="jmsType">
                      <xs:annotation>
                        <xs:documentation>A message JMSType identifier supplied by a client when a message is sent.</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                  </xs:extension>
                </xs:complexContent>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
          <xs:attribute type="xs:string" use="optional" name="encoding">
            <xs:annotation>
              <xs:documentation>The default encoding of the Message body to be used if the message doesn't communicate it</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="optional" default="*/*" name="contentType">
            <xs:annotation>
              <xs:documentation>The default contentType of the Message body to be used if the message doesn't communicate it</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/jms" type="org.mule.runtime.extension.api.runtime.parameter.OutboundCorrelationStrategyEnumType" use="optional" default="AUTO" name="sendCorrelationId">
            <xs:annotation>
              <xs:documentation>Whether to specify a correlationId when publishing messages. This applies both for custom correlation ids specifies at the operation level and for default correlation Ids taken from the current event</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="required" name="name">
            <xs:annotation>
              <xs:documentation>The identifier of this element used to reference it in other components</xs:documentation>
            </xs:annotation>
          </xs:attribute>
